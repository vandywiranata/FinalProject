<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="mainpane" prefHeight="200" prefWidth="320" stylesheets="@LoginCSS.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="loginapp.LoginMainController">
   <children>
      <Label fx:id="dbstatus" layoutX="7.0" layoutY="7.0" prefHeight="25.0" prefWidth="308.0" />
      <TextField fx:id="username" layoutX="113.0" layoutY="17.0" promptText="Username" />
      <PasswordField fx:id="password" layoutX="114.0" layoutY="58.0" promptText="Password" />
      <ComboBox fx:id="combobox" layoutX="114.0" layoutY="100.0" prefHeight="31.0" prefWidth="187.0" promptText="select" />
      <Button fx:id="loginButton" layoutX="114.0" layoutY="146.0" mnemonicParsing="false" onAction="#Login" prefHeight="31.0" prefWidth="187.0" stylesheets="@LoginCSS.css" text="Login" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
